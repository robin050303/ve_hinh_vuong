   "source": [
    "import turtle\n",
    "a = int(input(\"Nhập độ dài cạnh hình vuông: \"))\n",
    "t = turtle.Turtle()\n",
    "t.hideturtle()\n",
    "t.pencolor(\"red\")\n",
    "edge = 0\n",
    "while edge < 4:\n",
    "    t.fd(a)\n",
    "    t.rt(90)\n",
    "    edge += 1\n",
    "\n",
    "turtle.done()"
   ]
  }
